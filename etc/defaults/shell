
. /environment
if test -n "$\SHELL" -a -x "\$SHELL"; then
    exec "\$SHELL" "\$@"
else
    echo "ERROR: Shell does not exist in container: \$SHELL" 1>&2
    echo "ERROR: Using /bin/sh instead..." 1>&2
fi
if test -x /bin/sh; then
    SHELL=/bin/sh
    export SHELL
    exec /bin/sh "\$@"
else
    echo "ERROR: /bin/sh does not exist in container" 1>&2
fi
exit 1